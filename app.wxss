/* 全局引入iconfont样式 */
@import './styles/iconfont.wxss'

/* 初始化全局样式 */
page, view, text, swiper, swiper-item, image, navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/* 全局变量定义 设置主题颜色 字体大小 */
page {
  /* 主题颜色 */
  --themeColor: #1296db;
  /* 字体大小 */
  font-size: 28rpx;
}